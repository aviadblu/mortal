{
  "compilerOptions": {
    "target": "es6",
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "postgres",
    "web-server",
    "node_modules",
    "dist"
  ]
}